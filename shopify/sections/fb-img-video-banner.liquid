{%- style -%}
  .fbhomebaner-swiper-pagination .swiper-pagination-bullet {
    background: #979797;
    border: 1px solid #979797;
    opacity: 1;
  }
  .fbhomebaner-swiper-pagination .swiper-pagination-bullet-active {
    background: #121212 !important;
    border: 1px solid #121212 !important;
  }
  .fb-newhome-baner{
     font-family: 'Barlow', sans-serif;
  }
  /* .fbhomebaner-swiper-pagination {
    width: 100%;
    bottom: 20px;
    padding-left: 20px;
    position: absolute;
    z-index: 2;
    text-align: left;
  } */
  @media screen and (max-width: 1200px) {
    .fbhomebaner-swiper-pagination {
      width: fit-content;
      bottom: 10px!important;
      left: 50%!important;
      transform: translateX(-50%)!important;
      text-align: center;
      position: absolute;
      z-index: 2;
      width: auto!important;
      border-radius: 10px;
      min-width: 58px;
      display: flex;
      justify-content: center;
      padding: 2px 1px;
      background-color: rgba(255, 255, 255, 0.6);
    }
  }
  .fullscreen {
    height: calc(var(--window-height) - var(--header-height) - var(--announcement-bar-height, 0px));
    max-height: 100vh;
  }

  .line-shu{
    width:1px;
    background: #FDE7A0;
    height:1rem;
    margin:0 30px;
  }

{%- endstyle -%}
<section
  id="section-{{ section.id }}"
  data-section-id="{{ section.id }}"
  class="fb-newhome-baner min-h-12.5r"
  data-type="swiper"
>
  <div class="fbHomeImgVideoBanner relative overflow-hidden {% if section.settings.show_fullscreen == false %}{% else %} fullscreen{% endif %}">
    <div class="swiper-wrapper w-full overflow-hidden">
      {%- assign loopIndex = 1 -%}
      {%- for block in section.blocks -%}
        {%- capture slide_image_content -%}
            {%- assign mobile_image = block.settings.mobile_image | default: block.settings.image -%}
            {%- assign mobile_image_new = mobile_image | img_url: '' -%}
            {%- if mobile_image -%}
              <div class="h-full w-full fb-sm:hidden" style="{% unless section.settings.show_fullscreen %}--aspect-ratio: {{ mobile_image.aspect_ratio }};{% endunless %} background-image: url({{ mobile_image | img_url: '1x1', format: 'jpg' }})">
                <img
                  {% if loopIndex == 1 %}
                    class="h-full w-full object-cover"
                    fetchpriority="high"
                    src="{{ mobile_image_new }}"
                  {% else %}
                    class="h-full w-full object-cover lazyload"
                    data-src="{{ mobile_image_new }}"
                  {% endif %}
                  alt="{{ block.settings.mobile_image.alt | escape }}"
                  data-sizes="auto"
                  width="100%"
                  height="100%"
                  data-optimumx="1.2"
                  data-widths="[400, 600, 800, 1000, 1200, 1400, 1600, 1800, 2000, 2200]"
                >
              </div>
            {%- endif -%}
            <div class="hidden h-full w-full fb-sm:block" 
              style="background-image: url({{ block.settings.image | img_url: '1x1', format: 'jpg' }}); background-size: cover;">
              {% assign image_url = block.settings.image | img_url: '' %}
              <img
                {% if loopIndex == 1 %}
                  class="h-full w-full object-cover"
                  fetchpriority="high"
                  src="{{ image_url }}"
                {% else %}
                  class="h-full w-full object-cover lazyload"
                  data-src="{{ image_url }}"
                {% endif %}
                alt="{{ block.settings.image.alt | escape }}"
                data-sizes="auto"
                width="100%"
                height="100%"
                data-optimumx="1.2"
                data-widths="[400, 600, 800, 1000, 1200, 1400, 1600, 1800, 2000, 2200]"
              >
            </div>
          {%- endcapture -%}
        {%- capture slide_video_content -%}
          {%- assign mobile_vedio = block.settings.video_mobile_url | default: block.settings.video_url -%}
            <div class="h-full w-full fb-sm:hidden">
              <video
                class="h-full w-full object-cover"
                x-webkit-airplay="allow"
                webkit-playsinline="true"
                playsinline="true"
                autoplay="autoplay"
                muted="muted"
                loop="loop"
                src="{{ mobile_vedio }}"
                alt="{{ mobile_vedio.alt | escape }}"></video>
            </div>
            <div class="hidden h-full w-full fb-sm:block">
              <video
                class="h-full w-full object-cover"
                x-webkit-airplay="allow"
                webkit-playsinline="true"
                playsinline="true"
                autoplay="autoplay"
                muted="muted"
                loop="loop"
                src="{{ block.settings.video_url }}"></video>
            </div>
          {%- endcapture -%}
        {%- capture slideshow_text_content -%}
            {%- render 'fb-img-video-text3', block: block, type: 'img' -%}
          {%- endcapture -%}
        {%- capture slideshow_text_box -%}
            {%- if slideshow_text_content != blank -%}
              {%- if block.settings.content_position_pc == 'middleLeft' -%}
                <div class="absolute top-0 hidden h-full w-full items-center px-20 text-start fb-sm:flex">
                  <header class="SectionHeader">
                    {{ slideshow_text_content }}
                  </header>
                </div>
              {%- else -%}
                  <div class="absolute flex flex-col items-center justify-center">
                    <header class="SectionHeader">
                      {{ slideshow_text_content }}
                    </header>
                  </div>
              {%- endif -%}
              {%- if block.settings.content_position_mobile == 'bottomLeft' -%}
                <div class="absolute bottom-7 justify-start px-5 fb-sm:hidden">
                  <header class="SectionHeader">
                    {{ slideshow_text_content }}
                  </header>
                </div>
              {%- else -%}
                <div class="absolute top-10 mx-auto w-full px-3 text-center fb-sm:hidden">
                  <header class="SectionHeader">
                    {{ slideshow_text_content }}
                  </header>
                </div>
              {%- endif -%}
              
            {%- endif -%}
          {%- endcapture -%}
        {%- capture slideshow_type_banner -%}
          {%- if block.type == 'image_banner' -%}
            {{- slide_image_content -}}
          {%- else -%}
            {{- slide_video_content -}}
          {%- endif -%}
          {{- slideshow_text_box -}}
        {%- endcapture -%}

        {%- capture country_banner -%}
          {%- if block.settings.button_1_text == blank and block.settings.banner_href -%}
            <a
              href="{{ block.settings.button_1_link }}"
              class="swiper-slide js-homebanner-slid js-banner-kinds relative h-full w-full"
              data-number="{{ loopIndex }}"
              data-type="{{ block.type }}"
            >
              {{- slideshow_type_banner -}}
            </a>
          {%- else -%}
            <div
              id="Slide{{ block.id }}"
              class="swiper-slide js-homebanner-slid js-banner-kinds relative h-full w-full"
              data-number="{{ loopIndex }}"
              data-type="{{ block.type }}"
            >
              {{- slideshow_type_banner -}}
            </div>
          {%- endif -%}
          {%- assign loopIndex = loopIndex | plus: 1 -%}
        {%- endcapture -%}
        {{- country_banner -}}
      {%- endfor -%}
    </div>
    {%- assign circle_hidden_css = 'hidden fb-md:flex' -%}
    {% if section.blocks.size <= 1 %}
      {%- assign circle_hidden_css = '!hidden !fb-md:hidden' -%}
    {% endif %}
    <div class="absolute h-9  pl-5 pr-5 w-full items-center justify-between space-x-3 z-1 {{ circle_hidden_css }}" style="bottom:50%;transform: translateY(50%);">
