                {%- render 'pc-sidebar', container_id: 'pc-account-otherlogin' -%}
              </div>
              <drawer-content id="mob-sidebar-accout" class="drawer drawer--from-right !w-[80vw]">
                <span class="drawer__overlay"></span>
                <button
                  type="button"
                  class="absolute top-5 right-4"
                  data-action="close"
                  title="{{ 'general.accessibility.close' | t | escape }}"
                >
                  {%- render 'icon' with 'close' -%}
                </button>
