        {%- if request.page_type == 'index' -%}
          <h1 class="flex items-center">{{ logo }}</h1>
        {%- else -%}
          <div class="flex items-center">{{ logo }}</div>
        {%- endif -%}
      </div>
      <div class="header__search-bar hidden fb-md:flex fb-md:mx-[72px] fb-md:justify-between fb-md:flex-1">
