<script>
  $(function() {
      new Vue({
        el: '#mobile-sidebar-section',
        delimiters: ['${', '}'],
        components: {
          mobileSidebar,
        },
      });
  });
  const headerElement = document.getElementById('shopify-section-{{ section.id }}'),
  headerHeight = headerElement.clientHeight,
  headerHeightWithoutBottomNav = headerElement.querySelector('.header__wrapper').clientHeight;
  document.documentElement.style.setProperty('--header-height', headerHeight + 'px');
  document.documentElement.style.setProperty('--header-height-without-bottom-nav', headerHeightWithoutBottomNav + 'px');

  function addPhoneLink(){
   // 查找电话图标
    const phoneIcon = document.querySelector('.phone-click-link');
    if (phoneIcon) {
      // 添加点击事件
      phoneIcon.addEventListener('click', function() {
        const phoneNumber = '1-800-805-6686';
        window.location.href = 'tel:' + phoneNumber;
      });
    }
  }

  document.addEventListener('DOMContentLoaded', function() {

  $('.enter-cart-btn').click(()=>{
    window.location.href = '/cart'
  })

    addPhoneLink()
    if (window.location.pathname === '/cart') {
      // 如果是购物车页面，直接返回，不添加hover事件
      return;
    }

    let cartCache = null;
    let isFetching = false;
    let fetchPromise = null;
    
    // 预加载购物车数据
    function fetchCartData() {
      if (!isFetching) {
        isFetching = true;
        fetchPromise = fetch(`${window.themeVariables.routes.cartUrl}.js`)
          .then(response => response.json())
          .then(cartContent => {
            cartCache = cartContent;
            isFetching = false;
            return cartContent;
          })
          .catch(error => {
            isFetching = false;
            console.error('获取购物车数据失败:', error);
          });
      }
      return fetchPromise;
    }
    
    // 页面加载完成后立即预加载
    // fetchCartData();

    const cartIcon = document.querySelector('#hover-mini-cart');
    if(cartIcon){
      // 鼠标进入图标区域
    cartIcon.addEventListener('mouseenter', async function() {
      // const cartResponse = await fetch(`${window.themeVariables.routes.cartUrl}.js`);
      // const cartContent = await cartResponse.json();
      $('.my-loading-spinner').removeClass('hidden');
      document.documentElement.dispatchEvent(
        new CustomEvent('cart:refresh', {
          bubbles: false,
          detail: {
            // cart: cartContent,
            openMiniCart: true,
            nextReplacementDelay:0 ,
            replacementDelay:0 
          },
        })
      );
      });
    }

    cartIcon.addEventListener('mouseleave', async function() {
      $('.my-loading-spinner').addClass('hidden');
    })
  })

</script>
