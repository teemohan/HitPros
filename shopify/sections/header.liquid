            {% if cart.item_count > 99 %}
              99+
            {% else %}
              {{- cart.item_count -}}
            {% endif %}
          </cart-count>
          <div class="flex items-center justify-start {% if customer.id %} text-main {% else %} text-button  {% endif %} enter-cart-btn" >
            <span>
              {%- render 'icon' with 'new-cart' -%}
            </span>
            <span class="hidden fb-md:inline text-16 text-main font-medium ml-1 "
            id="hover-mini-cart"
            >Cart</span>
          </div>
